        }
    },
    "paths": {
       "/categories": {
        "post": {
            "tags": ["categories"],
            "summary": "Create a category",
            "description": "Create a new category",
            "requestBody": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties":{
                                "name": {
                                    "type": "string"
                                },
                                "description": {
                                    "type": "string"
                                }
                            },
                            "example":{
                                "name": "category name example",
                                "description": "category description example"
                            }
                        }
                    }
                }
            },
            "responses": {
                "201": {
                    "description": "Created"
                },
                "500": {
                    "description": "Category already exists."
                }
            }
        },
        "get": {
            "tags": ["categories"],
            "summary": "List all categories",
            "description": "List all categories",
            "responses": {
                "200": {
                    "description": "success",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "description": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
       }
    }
}